                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select  PT.PAYMENTID, 
		DO_TIME.ID as ID_DO_TIME, 
		PU_TIME.ID as ID_PU_TIME, 
		V.VENDORID, 
		LO_PICK.LONGTITUDE as 'PICKUP_LONGTITUDE',
		LO_PICK.LATITUDE as 'PICKUP_LATITUDE',
		LO_DROP.LONGTITUDE as 'DROPOFF_LONGTITUDE',
		LO_DROP.LATITUDE as 'DROPOFF_LATITUDE',
		R.RATECODEID, 
		SUM(PASSENGER_COUNT) as TOTAL_PASSENGER_COUNT,
		SUM(TRIP_DISTANCE) as TOTAL_TRIP_DISTANCE,
		SUM(FARE_AMOUNT) as TOTAL_FARE_AMOUNT,
		SUM(EXTRA) as TOTAL_EXTRA,
		SUM(TIP_AMOUNT) as TOTAL_TIP_AMOUNT,
		SUM(TOLLS_AMOUNT) as TOTAL_TOLLS_AMOUNT,
		SUM(TOTAL_AMOUNT) as TOTAL_AMOUNT,
                                     COUNT(*) as TRIP_COUNT
from ((((((DDS_Taxi.dbo.time_dim as DO_TIME left join Trip as T 
		on (DO_TIME.GIO = DATEPART(HOUR, T.dropoff_datetime) and
			DO_TIME.NGAY = Day(T.dropoff_datetime) and
			DO_TIME.THANG = Month(T.dropoff_datetime) and
			DO_TIME.NAM = Year(T.dropoff_datetime))) right join DDS_Taxi.dbo.time_dim as PU_TIME
		on (PU_TIME.GIO = DATEPART(HOUR, T.pickup_datetime) and
			PU_TIME.NGAY = Day(T.pickup_datetime) and
			PU_TIME.THANG = Month(T.pickup_datetime) and
			PU_TIME.NAM = Year(T.pickup_datetime))) right join LOCATIONS as LO_PICK
		on (LO_PICK.ID = T.PU_LOC_ID)) right join LOCATIONS as LO_DROP
		on (LO_DROP.ID = T.DO_LOC_ID)) inner join PAYMENT_TYPE as PT 
		on (PT.ID = T.PAYMENT_ID)) inner join VENDOR as V
		on (V.ID = T.VENDOR_ID)) inner join RATE_CODE as R
		on (R.ID = T.RAT_ID)
GROUP BY PT.PAYMENTID, 
		 DO_TIME.ID, 
		 PU_TIME.ID, 
		 V.VENDORID, 
		 LO_PICK.LONGTITUDE,
		 LO_PICK.LATITUDE,
